<h1>This is a working map page</h1>
<div id="map" style="width:100%;height:400px;"></div>

<script>
// Initialize and add the map
function initMap() {
  var savannah = { lat: 32.0809, lng: -81.0912};
  var map = new google.maps.Map(
      document.getElementById('map'), {zoom: 8, center: savannah});
 
  var marker = new google.maps.Marker({position: savannah, map: map});
}
</script>
  
<script async defer
src="https://maps.googleapis.com/maps/api/js?key=[process.env.google_ID]&callback=initMap">
</script>